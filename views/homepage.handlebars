<!-- Loop over each object in the projects array -->
<h2 class="event-list">Event Listings</h2>
{{#each data.events as |event| }}
<div class="row">
<div class="column mb-4 project">
  <div class="cards col-md-5">
    <h3 class="cards-container">
      <a href="/event/{{event.id}}">{{event.title}}</a>
    </h3>
  </div>
</div>
</div>
{{/each}}


