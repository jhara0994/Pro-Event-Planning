  <h3 class="event-title">Title: {{events.title}}</h3>
  <p class="event-id">ID: {{events.id}}</p>
  <p class="event-date">Date: {{formattedDate}}</p>
  <p class="weather-icon">Weather: {{{get_display_displayIcon}}}</p>
  <p class="event-desc">Description: {{events.event_description}}</p>
  <p class="event-creator">Created by: User {{events.user.name}}</p>
  <img class="image" source={{photo.image}} aria-label="photo">Photo: </p>
  <p class="event-price">Registration Fee: ${{events.fee}} </p>
  <p>
    <button href="/registration/{{reg.id}}" class="btn btn-primary">Register Now</button>
  </p>
  </div>
</div>
